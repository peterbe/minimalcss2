{
	"name": "minimalcss2",
	"version": "0.0.1",
	"description": "Like critters but on steroids",
	"keywords": [
		"css",
		"criticalcss",
		"critical",
		"minimal",
		"minimalcss"
	],
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"test": "npm run test:vitest && npm run lint",
		"test:vitest": "vitest",
		"lint": "npx @biomejs/biome check",
		"format": "npx @biomejs/biome check --write --unsafe",
		"build": "tsc",
		"prepare": "npm run build",
		"postinstall": "npx @biomejs/biome migrate --write"
	},
	"author": "Peter Bengtsson",
	"license": "MIT",
	"dependencies": {
		"cheerio": "1.1.2",
		"css-tree": "2.3.1",
		"csso": "5.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/css-tree": "2.3.1",
		"@types/csso": "5.0.0",
		"@types/jest": "29.5.3",
		"@types/node": "24.10.1",
		"jest": "30.2.0",
		"jest-junit": "16.0.0",
		"ts-jest": "29.4.6",
		"typescript": "5.9.3",
		"vitest": "^4.0.15"
	}
}
